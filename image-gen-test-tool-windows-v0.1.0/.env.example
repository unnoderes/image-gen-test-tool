# Alibaba
ALIBABA_API_KEY=
ALIBABA_REGION=intl
# Optional override; defaults to region host + official path.
ALIBABA_TEXT2IMAGE_URL=
ALIBABA_IMAGE2IMAGE_URL=
ALIBABA_ASYNC=false
ALIBABA_ASYNC_URL=
ALIBABA_POLL_INTERVAL_SECONDS=10
ALIBABA_POLL_TIMEOUT_SECONDS=300

# Google
GOOGLE_API_KEY=
# Optional override; defaults to Gemini API generateContent template.
GOOGLE_TEXT2IMAGE_URL=
GOOGLE_IMAGE2IMAGE_URL=

# GLM (Zhipu / BigModel)
GLM_API_KEY=
# Optional override; defaults to https://open.bigmodel.cn/api/paas/v4
GLM_BASE_URL=
# Optional override; defaults to {GLM_BASE_URL}/images/generations
GLM_TEXT2IMAGE_URL=
GLM_IMAGE2IMAGE_URL=

# Optional
HTTP_TIMEOUT_SECONDS=120
MAX_RETRIES=1
RETRY_DELAY_SECONDS=2
IGT_OUTPUT_DIR=runs
# Alias format for downloaded .bin files from URL images: png or jpg.
IGT_BIN_ALIAS_FORMAT=png
# Enable/disable Alibaba image_to_image auto-crop (on/off).
IGT_ALIBABA_IMAGE2IMAGE_AUTOCROP=off
# Persist Alibaba image_to_image auto-cropped input into each run folder (on/off).
IGT_PERSIST_PREPROCESSED_INPUT=off
# Optional: custom model registry file path (defaults to ./custom_models.json).
IGT_CUSTOM_MODELS_PATH=custom_models.json
